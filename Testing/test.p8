-- jumpway v.1.0
-- by peter antoine

function _init()
	gs_show_splashscreen = 0
	gs_show_info = 1	
    gs_playing = 2
	gs_game_over = 3

    info_text={
	[[




  collect as many coins as
  you can while you dodge 
  the cars in traffic.


    ⬅️,➡️,⬆️,⬇️ to move
]]}

	txt_version = "v1.0"
    txt_by = "by"
    txt_author = "peter antoine"
	txt_start = "❎ to start"
	
	initialize()
	gs = gs_show_splashscreen
end

function initialize()
	cars = {}	
	coin = nil	
	px,py= 60,120	
	rc = 5 
	dcs, ccs=0,0

	t = time()
	time_started = t
	time_stopped = time_started

    timer_move_car = t           
    timer_new_car = t           
	timer_coin = t           
	
	speed_move_car = 0.1
	speed_new_car = 0.4
	speed_coin = 1	
end

function _update()
	if (gs == gs_playing) then      
		speed_new_car = get_rnd_speed()

		if (is_interval_passed(timer_move_car, speed_move_car)) then
			move_cars()
			timer_move_car = time()		
		end
		
		if (is_interval_passed(timer_new_car, speed_new_car)) then
			add_new_car()
			timer_new_car = time()		
		end
		
		if (is_interval_passed(timer_coin, speed_coin)) then
			add_new_coin()		
			timer_coin = time()		
		end

		if (btnp(⬅️) and px >= 60) then px-=10 			
		elseif (btnp(➡️) and px <= 60) then px+=10	
		elseif (btnp(⬆️)) then py-=10
		elseif (btnp(⬇️) and py < 120) then py+=10
		end

		if (coin != nil and py == coin[1] and px == coin[2]) then
			ccs+=1
			coin = nil	
			sfx(1)
		end
		for i=1,#cars  do
			if (cars[i][1] == py and cars[i][2] == px) then
				sfx(0)
				gs = gs_game_over 
			end
		end

	elseif (gs == gs_game_over) then      
		if (time_stopped == time_started) then
			time_stopped = time() - time_started
		end		
		if (btnp(❎)) then 
			initialize()		
			gs = gs_playing	
		end

	elseif (gs == gs_show_splashscreen) then
        if (btnp(❎)) then
            gs = gs_show_info            
        end        

    elseif (gs == gs_show_info) then
        if (btnp(❎)) then
			initialize()		
			gs = gs_playing	
		end        	
	end     
end

function _draw()
	if (gs == gs_playing) then		
		-- sky
		cls(rc)	
		rectfill(0,0,127,10,12)
		circfill(63, 14, 14, 10)
		rectfill(0,10,127,40,rc)
		-- road
		line(57,10, 0,112,7)
		line(69,10, 127,112,7)
		-- trees	
		local ty = 110	
		for	i=1, 13 do 
			spr(2,40,ty)	
			spr(2,80,ty) 
			ty-=20	
		end
		-- coin
		if (coin != nil) spr(8,coin[2],coin[1]) 
		-- player
		spr(1,px,py) 
		-- cars		
		for i=1, #cars do spr(cars[i][3],cars[i][2],cars[i][1]) end
		print("coins:"..ccs,1,1,9)
	elseif (gs == gs_game_over) then		
		draw_game_over()
    elseif (gs == gs_show_info) then draw_info()        
    elseif (gs == gs_show_splashscreen) then draw_splashscreen()		
	end
end

function draw_splashscreen()
    cls(13)               
    draw_bevelled_box(32,33,95,79,1)
    draw_shadow_text('jumpaway',46,38,9)       
    print(txt_version, hcenter(txt_version)-2,49,6)    
    print(txt_by, hcenter(txt_by)-2,60,5)    
	print(txt_author, hcenter(txt_author)-1,70,5)    
	draw_start()	
end

function draw_info()
    cls(13)    
    draw_bevelled_box(8,22,120,100,1)    
    draw_shadow_text("info",55,10,7)    
    print(info_text[1],11,20,9)    
    draw_start()
end

function draw_start()
    draw_bevelled_box(30,112,95,120,1)    
    print(txt_start, hcenter(txt_start)-3,114,6)	
end

function draw_game_over()	
    cls(13)    
    draw_bevelled_box(8,22,120,100,1)    	
	print("game over",45,35,8)  	
	print(ccs.." coins collected" ,32,53,7) print(tostr(ccs),32,53,11)		
	print(to_clock(flr(time_stopped)),32,63,11)	print("played",67,63,7)
	print(dcs.." dodged cars",32,73,7) print(tostr(dcs),32,73,11)	
    draw_start()
end

function draw_bevelled_box(x1, y1, x2, y2, c)
    rectfill(x1,y1,x2,y2,c)
    line(x1,y1-1,x2,y1-1,5)
    line(x1,y2+1,x2,y2+1,6)
    line(x1-1,y1,x1-1,y2,6)
    line(x2+1,y1,x2+1,y2,5)
end

function draw_shadow_text(s, x, y, c)
    print(s,x+1,y+1,5)
    print(s,x,y,c)
end

function move_cars()
	for i=#cars, 1, -1  do
		if (cars[i][1] < 120) then
			cars[i][1] += 10
		else
			del(cars, cars[i])
			dcs+=1
		end	
	end
end

function add_new_car()
	local r = get_rnd_lane()
	local c = get_rnd_car()
	add(cars, {-190, 50+r*10, c})	
end	

function add_new_coin()
	if (coin == nil) then
		local r = get_rnd_lane()
		local y = get_rnd_y()		
		coin = {120-y*20, 50+r*10}	
	end
end

function is_interval_passed(timer, speed)
    return time() - timer > speed
end

function to_clock(sec)
	if (sec <= 0) return "00:00:00"	
	local h = lpad(flr(sec/3600))
	local m = lpad(flr(sec/60-(h*60)))
	local s = lpad(flr(sec-h*3600-m*60))
	return h..":"..m..":"..s	
end

function lpad(n)    
    if (n < 10) return "0"..n
    return ""..n;
end

function get_rnd_y()   
   return  flr(rnd(5)+1)
end

function get_rnd_lane()
    return  flr(rnd(3))
end

function get_rnd_car()
    return  flr(rnd(5)+3)
end

function get_rnd_speed()
    return  flr(rnd(5)+2)/10
end

function wait(a) 
    for i=1, a do 
        flip() 
    end 
end

function hcenter(s)
	return 64-#s*2
end

-- Instant3D by @mot
_={
	map=map,
	spr=spr,
	sspr=sspr,
	pset=pset,
	circ=circ,
	camera=camera,
	cx=0,
	cy=0,
	cyoff=32,
	ch=32,
	d=128,
	ymid=0,
	s2w=function(x,y)
	 return (x-_.cx)-64,_.ch,128-(y-_.cy)
	end,
	proj=function(x,y,z)
	 local scale=_.d/z
	 return x*scale+64,y*scale+_.ymid,scale
	end
   }
   
   camera=function(x,y)
	_.cx=x or 0
	_.cy=(y or 0)+_.cyoff
   end
   
   camera(0,0)
   
   pset=function(x,y,c)
	local wx,wy,wz=_.s2w(x,y)
	if(wz<1)return
	local px,py=_.proj(wx,wy,wz)
	_.pset(px,py,c)
   end
   
   circ=function(x,y,r,c)
	local wx,wy,wz=_.s2w(x,y)
	if(wz<1)return
	local px,py,scale=_.proj(wx,wy,wz)
	_.circ(px,py,r*scale,c)
   end
   
   sspr=function(sx,sy,sw,sh,x,y,w,h,fx,fy)
	w=w or sw
	h=h or sh
	local wx,wy,wz=_.s2w(x+w/2,y+h)
	if(wz<1)return
	local px,py,scale=_.proj(wx,wy,wz)
	local pw,ph=w*scale,h*scale
   
	-- sub pixel stuff
	local x0,x1=flr(px-pw/2),flr(px+pw/2)
	local y0,y1=flr(py-ph),flr(py)
   
	_.sspr(sx,sy,w,h,x0,y0,x1-x0,y1-y0,fx,fy)
   end
   
   spr=function(n,x,y,w,h,fx,fy)
	if(not n or not x or not y)return
	-- convert to equivalent sspr() call
	w=(w or 1)*8
	h=(h or 1)*8
	local sx,sy=flr(n%16)*8,flr(n/16)*8
	sspr(sx,sy,w,h,x,y,w,h,fx,fy)
   end
   
   map=function(cx,cy,x,y,w,h,lyr)
	cx=cx or 0
	cy=cy or 0
	x=x or 0
	y=y or 0
	w=w or 128
	h=h or 64
   
	-- near/far corners
	   local fx,fy,fz=_.s2w(x,y)
	   local nx,ny,nz=_.s2w(x,y+h*8)
   
	-- clip
	nz=max(nz,0.5)
	if(fz<=nz)return
   
	-- project
	local npx,npy,nscale=_.proj(nx,ny,nz)
	local fpx,fpy,fscale=_.proj(fx,fy,fz)
   
	-- clamp
	npy=min(npy,128)
   
	-- rasterise
	local py=flr(npy)
	while py>=fpy do
   
	 -- floor plane intercept
		   local g=(py-_.ymid)/_.d
		   local z=_.ch/g      
   
		   -- map coords
		   local mx,my=cx,(fz-z)/8+cy
   
		   -- project to get left/right
		   local lpx,lpy,lscale=_.proj(nx,ny,z)
		   local rpx,rpy,rscale=_.proj(nx+w*8,ny,z)
   
		   -- delta x
		   local dx=w/(rpx-lpx)
   
		   -- sub-pixel correction
		   local l,r=flr(lpx+0.5)+1,flr(rpx+0.5)
		   mx+=(l-lpx)*dx
   
	 -- render
		   tline(l,py,r,py,mx,my,dx,0,lyr)
   
	 py-=1
	end 
   end




